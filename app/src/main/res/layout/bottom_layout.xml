<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="5dp"
            android:orientation="horizontal">




            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">


            </LinearLayout>
            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">




            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:gravity="center"
                android:elevation="5dp"
                android:background="@drawable/home_radius_button_white"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_marginTop="16dp"
                    android:src="@drawable/double_arrow"></ImageView>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:textSize="12sp"
                    android:layout_marginTop="4dp"

                    android:textAllCaps="true"
                    android:text="grocery"
                    android:textColor="#43833E" />
            </LinearLayout>
            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">


            </LinearLayout>




            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">


            </LinearLayout>
        </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="6dp"
        android:paddingBottom="6dp"
        android:layout_marginTop="16dp"
        android:background="@color/colorWhite"
        android:elevation="5dp"
        android:orientation="horizontal">




    <LinearLayout
        android:id="@+id/homeBottomLL"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/homeIV"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/home"></ImageView>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="10sp"
            android:id="@+id/homeBottomTV"
            android:text="@string/homeText"
            android:layout_marginTop="4dp"
            android:singleLine="true"
            android:textAllCaps="true"
            android:textColor="#484848" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/reservationBottomLL"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/reservationIV"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/table"></ImageView>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="10sp"
            android:layout_marginTop="4dp"
            android:id="@+id/reservationBottomTV"
            android:text="@string/reservationText"
            android:textAllCaps="true"
            android:singleLine="true"
            android:textColor="#484848" />


    </LinearLayout>

        <LinearLayout
            android:id="@+id/groceryBottomLL"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">



            <ImageView
                android:id="@+id/groceryIV"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/grocery"></ImageView>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="10sp"
                android:layout_marginTop="4dp"
                android:id="@+id/groceryBottomTV"
                android:textAllCaps="true"
                android:text="grocery"
                android:textColor="#43833E" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/favoriteBottomLL"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/favoriteIV"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/favorite"></ImageView>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="10sp"
                android:layout_marginTop="4dp"
                android:id="@+id/favoriteBottomTV"
                android:textAllCaps="true"
                android:singleLine="true"
                android:text="@string/favoriteText"
                android:textColor="#484848" />
        </LinearLayout>




        <LinearLayout
            android:id="@+id/accountPassBottomLL"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"

            android:orientation="vertical">

            <ImageView
                android:id="@+id/accountIV"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/my_account"></ImageView>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:id="@+id/accountPassBottomTV"
                android:textAllCaps="true"
                android:singleLine="true"
                android:text="@string/AccountText"
                android:layout_marginTop="4dp"
                android:textSize="10sp"
                android:textColor="#484848" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>
</LinearLayout>